{
  "name": "vuetify-notifier",
  "version": "0.0.3",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/vuetify-notifier.umd.cjs",
  "module": "./dist/vuetify-notifier.js",
  "exports": {
    ".": {
      "import": "./dist/vuetify-notifier.js",
      "require": "./dist/vuetify-notifier.umd.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && npm run copy-nuxt",
    "preview": "vite preview",
    "copy-nuxt": "cp -r src/plugins/vuetify-notifier/nuxt dist"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.3.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vue-tsc": "^1.2.0",
    "vuetify": "^3.1.13"
  }
}
